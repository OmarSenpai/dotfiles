{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "name": "S9npai",
  "author": "Omar Mohammed",
  "themes": [
    {
      "name": "S9npai",
      "appearance": "dark",
      "style": {
        "background": "#2B2D30",
        "elevated_surface.background": "#22020",
        "surface.background": "#2B2D30",
        "panel.background": "#222222",
        "panel.focused_border": "#3474F0",
        "border": "#101010",
        "border.variant": "#393B41",
        "border.focused": "#3474F0",
        "border.selected": "#3474F0",
        "border.transparent": "#000000",
        "border.disabled": "#393B41",
        "element.background": "#2B2D30",
        "element.hover": "#2E436B",
        "element.active": "#0A3783",
        "element.selected": "#0A3783",
        "element.disabled": "#2B2D30",
        "drop_target.background": "#3474F050",
        "error.background": "#CC1212",
        "ghost_element.background": "#191919",
        "ghost_element.hover": "#2E436B",
        "ghost_element.active": "#0A3783",
        "text": "#BDBDBD",
        "text.muted": "#9FA1A5",
        "text.placeholder": "#9FA1A5",
        "text.disabled": "#9FA1A5",
        "text.accent": "#82ACFC",
        "icon": "#BDBDBD",
        "icon.muted": "#9FA1A5",
        "icon.disabled": "#9FA1A5",
        "icon.placeholder": "#BDBDBD",
        "icon.accent": "#82ACFC",

        "status_bar.background": "191919",
        "title_bar.background": "#191919",
        "toolbar.background": "#222222",
        "tab_bar.background": "#202020",
        "tab.inactive_background": "#2A2A2A",
        "tab.active_background": "#222222",
        "search.match_background": "#0A3783",

        "editor.background": "#252525",
        "editor.foreground": "#BDBDBD",
        "editor.gutter.background": "#252525",
        "editor.subheader.background": "#2B2D30",
        "editor.active_line.background": "#2B2D30",
        "editor.highlighted_line.background": "#2E436B40",
        "editor.line_number": "#9FA1A5",
        "editor.active_line_number": "#F1F1F1",
        "editor.invisible": "#BDBDBD",
        "editor.wrap_guide": "#393B41",
        "editor.active_wrap_guide": "#82ACFC",
        "editor.document_highlight.read_background": "#2E436B",
        "editor.document_highlight.write_background": "#0A3783",

        "players": [
          { "cursor": "#3474F0", "selection": "#0A3783" },
          { "cursor": "#85C46C", "selection": "#85C46C" },
          { "cursor": "#C8A16D", "selection": "#C8A16D" },
          { "cursor": "#EC94C0", "selection": "#EC94C0" },
          { "cursor": "#C191FF", "selection": "#C191FF" },
          { "cursor": "#3ACC9C", "selection": "#3ACC9C" },
          { "cursor": "#65C3CC", "selection": "#65C3CC" }
        ],

        "terminal.background": "#1F1F1F",
        "terminal.foreground": "#DBDBDB",
        "terminal.bright_foreground": "#ffffff",
        "terminal.ansi.black": "#262626",
        "terminal.ansi.bright_black": "#9FA1A5",
        "terminal.ansi.red": "#EC94C0",
        "terminal.ansi.bright_red": "#ff88c8",
        "terminal.ansi.green": "#85C46C",
        "terminal.ansi.bright_green": "#ADEB96",
        "terminal.ansi.yellow": "#C8A16D",
        "terminal.ansi.bright_yellow": "#ffca80",
        "terminal.ansi.blue": "#6C95EB",
        "terminal.ansi.bright_blue": "#82ACFC",
        "terminal.ansi.magenta": "#C191FF",
        "terminal.ansi.bright_magenta": "#E2BFFF",
        "terminal.ansi.cyan": "#65C3CC",
        "terminal.ansi.bright_cyan": "#7fffff",
        "terminal.ansi.white": "#BDBDBD",
        "terminal.ansi.bright_white": "#ffffff",

        "syntax": {
          "comment": { "color": "#878787" },
          "keyword": { "color": "#6C95EB" },
          "string": { "color": "#C8A16D" },
          "string.escape": { "color": "#BDBDBD" },
          "number": { "color": "#56CC9E" },
          "boolean": { "color": "#6C95EB" },
          "function": { "color": "#3ACC9C" },
          "type": { "color": "#C191FF" },
          "property": { "color": "#5997C0" },
          "enum": { "color": "#E2BFFF" },
          "constant": { "color": "#C191FF" },
          "variable": { "color": "#BDBDBD" },
          "variable.special": { "color": "#6C95EB" },
          "operator": { "color": "#BDBDBD" },
          "punctuation": { "color": "#BDBDBD" },
          "tag": { "color": "#C191FF" },
          "attribute": { "color": "#85C46C" },
          "constructor": { "color": "#C191FF" },
          "emphasis": { "font_style": "italic" },
          "emphasis.strong": { "font_weight": 700 },
          "link_uri": { "color": "#82ACFC", "font_style": "italic" },
          "link_text": { "color": "#82ACFC", "font_style": "normal" },
          "title": { "color": "#C191FF", "font_weight": 700 },
          "predictive": { "color": "#9FA1A5" }
        },

        "created": "#6fd181",
        "modified": "#4e77de",
        "conflict": "#cc1b1b",
        "error": "#d51515",
        "warning": "#c7c41a"
      }
    }
  ]
}
